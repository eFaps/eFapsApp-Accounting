<datamodel-sqltable  xmlns="http://www.efaps.org/xsd"
                     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                     xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">
    <uuid>882667da-b9b8-4435-964f-701ae50a7231</uuid>
    <file-application>eFaps-ERP-Accounting</file-application>
    <file-revision>$Rev$</file-revision>

    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Accounting_TransactionWithPosSQLView</name>
        <database>
            <view-name>V_ACCTRANSPOS</view-name>
            <sql>
                 create or replace view V_ACCTRANSPOS as
                 SELECT
                    t_acctransactionpos.id, status, date, descr, name, periodeid,
                      accountid,amount, currencyid, ratecurrencyid,rate
                    FROM
                      public.t_acctransaction join
                      public.t_acctransactionpos on t_acctransaction.id = t_acctransactionpos.transactionid;
            </sql>
        </database>
    </definition>
</datamodel-sqltable>
