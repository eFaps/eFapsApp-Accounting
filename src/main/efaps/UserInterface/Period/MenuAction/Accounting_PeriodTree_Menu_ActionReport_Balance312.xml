<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<ui-command xmlns="http://www.efaps.org/xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">
    <uuid>92bef665-f0ea-4f4d-9d7f-f686bbbda54f</uuid>
    <file-application>eFapsApp-Accounting</file-application>
    
    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Accounting_PeriodTree_Menu_ActionReport_Balance312</name>
        <access>
            <role>Accounting_Admin</role>
            <role>Accounting_Manager</role>
            <role>Accounting_Executive</role>
        </access>
        <target>
            <form>Accounting_PReportBalance312ReportForm</form>
            <execute program="org.efaps.esjp.common.jasperreport.StandartReport">
                <property name="JasperReport">Accounting_Balance312</property>
                <property name="DataSourceClass">org.efaps.esjp.accounting.report.balance.BalanceReport312DS</property>
                <property name="FileName">LIBRO INVENTARIOS Y BALANCES 3.12</property>
            </execute>
            <validate method="validateDates" program="org.efaps.esjp.accounting.report.Report"/>
            <trigger program="org.efaps.esjp.accounting.Period"
                method="accessCheckByConfig"
                name="Accounting_PeriodTree_Menu_ActionReport_Balance312.UI_ACCESSCHECK"
                event="UI_ACCESSCHECK">
                <property name="CheckOnContains">true</property>
                <property name="Key">ReportBalance312Account</property>
            </trigger>
        </target>
        <property name="Target">modal</property>
        <property name="TargetMode">create</property>
        <property name="TargetShowFile">true</property>
        <property name="NoUpdateAfterCOMMAND">true</property>
    </definition>
</ui-command>
