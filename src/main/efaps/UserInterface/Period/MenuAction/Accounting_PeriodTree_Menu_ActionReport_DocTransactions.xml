<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<ui-command xmlns="http://www.efaps.org/xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">

    <uuid>048204e7-a97e-4bc5-913e-e75152a390aa</uuid>
    <file-application>eFapsApp-Accounting</file-application>
    

    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Accounting_PeriodTree_Menu_ActionReport_DocTransactions</name>

        <access>
            <role>Accounting_Admin</role>
            <role>Accounting_Manager</role>
            <role>Accounting_Executive</role>
        </access>
        <target>
            <form>Accounting_DocTransactionsForm</form>
            <execute program="org.efaps.esjp.common.jasperreport.StandartReport">
                <property name="JasperReport">Accounting_DocTransactions</property>
                <property name="DataSourceClass">org.efaps.esjp.accounting.report.DocTransactionsSource </property>
            </execute>
            <validate program="org.efaps.esjp.accounting.report.Report" method="validateDates"/>
        </target>
        <property name="Target">modal</property>
        <property name="TargetMode">create</property>
        <property name="TargetShowFile">true</property>
        <property name="NoUpdateAfterCOMMAND">true</property>
        <property name="WindowWidth">800</property>
    </definition>
</ui-command>
