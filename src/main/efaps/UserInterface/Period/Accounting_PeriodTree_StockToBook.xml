<?xml version="1.0" encoding="UTF-8"?>




<ui-command xmlns="http://www.efaps.org/xsd"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">

    <uuid>f78db4c4-12f5-4bce-9c4a-6ae30653a602</uuid>
    <file-application>eFapsApp-Accounting</file-application>
    
    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Accounting_PeriodTree_StockToBook</name>
        <access>
            <role>Accounting_Admin</role>
            <role>Accounting_Manager</role>
        </access>
        <target>
            <menu>Accounting_PeriodTree_StockToBook_Menu</menu>
            <table>Accounting_PeriodDocumentTable</table>
            <evaluate program="org.efaps.esjp.accounting.Period"
                method="getStockToBook">
                <property name="Type">ERP_DocumentAbstract</property>
            </evaluate>
            <trigger program="org.efaps.esjp.admin.access.AccessCheck4UI"
                method="configObjectCheck"
                name="Accounting_PeriodTree_StockToBook_Menu.UI_ACCESSCHECK" event="UI_ACCESSCHECK">
                <property name="SystemConfig">Accounting-Configuration</property>
                <property name="Key">ActivateStock</property>
            </trigger>
        </target>
        

        <property name="Target">content</property>
        <property name="TargetShowCheckBoxes">true</property>
    </definition>
</ui-command>
