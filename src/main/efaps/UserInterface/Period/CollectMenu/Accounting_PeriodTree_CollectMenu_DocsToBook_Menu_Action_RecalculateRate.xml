<?xml version="1.0" encoding="UTF-8"?>




<ui-command xmlns="http://www.efaps.org/xsd"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">

    <uuid>55f1d618-a00d-4916-8ba9-c47a1b5c7d24</uuid>
    <file-application>eFapsApp-Accounting</file-application>
    

    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Accounting_PeriodTree_CollectMenu_DocsToBook_Menu_Action_RecalculateRate</name>

        <access>
            <role>Accounting_Admin</role>
            <role>Accounting_Manager</role>
        </access>
        <target>
            <form>Accounting_TransactionRecalculateForm</form>
            <execute program="org.efaps.esjp.accounting.transaction.Recalculate"
                method="recalculateRate"/>
            <trigger program="org.efaps.esjp.admin.access.AccessCheck4UI"
                method="check4Status"
                name="Accounting_PeriodTree_DocsToBook_Menu_Action_RecalculateRate.UI_ACCESSCHECK"
                event="UI_ACCESSCHECK">
                <property name="Status">Open</property>
            </trigger>
        </target>
        

        <property name="Target">modal</property>
        <property name="TargetCmdRevise">true</property>
        <property name="TargetMode">create</property>
        <property name="TargetCreateType">Accounting_Transaction</property>
        <property name="Submit">true</property>
    </definition>
</ui-command>
