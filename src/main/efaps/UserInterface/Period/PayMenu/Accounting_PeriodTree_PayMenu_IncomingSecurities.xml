<?xml version="1.0" encoding="UTF-8" standalone="no"?>


<ui-command xmlns="http://www.efaps.org/xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">
    <uuid>c3f61e85-34ca-45bd-9aa6-4a71bdd02d1d</uuid>
    <file-application>eFapsApp-Accounting</file-application>
    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Accounting_PeriodTree_PayMenu_IncomingSecurities</name>
        <access>
            <role>Accounting_Admin</role>
            <role>Accounting_Manager</role>
        </access>
        <target>
            <menu>Accounting_PeriodTree_PayMenu_IncomingSecurities_Menu</menu>
            <table>Accounting_PeriodSecuritiesTable</table>
            <evaluate program="org.efaps.esjp.common.uitable.MultiPrint">
                <property name="Type01">Sales_IncomingCredit</property>
                <property name="StatusGroup01">Sales_IncomingCreditStatus</property>
                <property name="Status01">Open</property>
                <property name="StatusGroup02">Sales_IncomingCreditStatus</property>
                <property name="Status02">Closed</property>
                <property name="StatusGroup03">Sales_IncomingCreditStatus</property>
                <property name="Status03">Canceled</property>
            </evaluate>
            <trigger event="UI_ACCESSCHECK" method="configObjectCheck"
                name="Accounting_PeriodTree_Securities.UI_ACCESSCHECK"
                program="org.efaps.esjp.admin.access.AccessCheck4UI">
                <property name="SystemConfig">Accounting-Configuration</property>
                <property name="Key">ActivateSecurities</property>
            </trigger>
        </target>
        

        <property name="Target">content</property>
        <property name="TargetShowCheckBoxes">true</property>
    </definition>
</ui-command>
