<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<ui-command xmlns="http://www.efaps.org/xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">
    <uuid>12070d24-bd35-4914-be36-a97be9b652df</uuid>
    <file-application>eFapsApp-Accounting</file-application>
    
    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Accounting_PeriodTree_PayMenu_Exchange</name>
        <access>
            <role>Accounting_Admin</role>
            <role>Accounting_Manager</role>
        </access>
        <target>
            <menu>Accounting_PeriodTree_PayMenu_Exchange_Menu</menu>
            <table>Accounting_PeriodExchangeTable</table>
            <evaluate program="org.efaps.esjp.accounting.Period" method="multiPrint" >
                <property name="Type01">Sales_Exchange</property>
            </evaluate>
            <trigger program="org.efaps.esjp.admin.access.AccessCheck4UI"
                method="configObjectCheck"
                name="Accounting_PeriodTree_Exchange.UI_ACCESSCHECK" event="UI_ACCESSCHECK">
                <property name="SystemConfig">Accounting-Configuration</property>
                <property name="Key">ActivateExchange</property>
            </trigger>
        </target>
        

        <property name="Target">content</property>
        <property name="TargetShowCheckBoxes">true</property>
    </definition>
</ui-command>
