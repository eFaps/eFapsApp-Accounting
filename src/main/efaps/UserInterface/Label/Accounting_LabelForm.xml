<?xml version="1.0" encoding="UTF-8"?>




<ui-form xmlns="http://www.efaps.org/xsd"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">
    <uuid>3de61841-8ce6-400f-a44e-110896d067f8</uuid>
    <file-application>eFapsApp-Accounting</file-application>
    
    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Accounting_LabelForm</name>
        

        <field name="type">
            <property name="Attribute">Type</property>
            <property name="ModeEdit">NONE</property>
            <property name="ModeView">NONE</property>
            <property name="ModePrint">NONE</property>
        </field>
        <field name="name">
            <property name="ModeCreate">EDITABLE</property>
            <property name="ModeEdit">EDITABLE</property>
            <property name="ModeSearch">EDITABLE</property>
            <property name="Attribute">Name</property>
            <property name="Required">true</property>
        </field>
        <field name="description">
            <property name="ModeCreate">EDITABLE</property>
            <property name="ModeEdit">EDITABLE</property>
            <property name="ModeSearch">EDITABLE</property>
            <property name="Attribute">Description</property>
            <property name="Required">true</property>
        </field>
        <field name="periodLink">
            <property name="SelectAlternateOID">linkto[PeriodAbstractLink].oid</property>
            <property name="Select">linkto[PeriodAbstractLink].attribute[Name]</property>
            <property name="Label">Accounting_LabelAbstract/PeriodLink.Label</property>
            <property name="HRef">${COMMONURL}/MenuTree.jsp</property>
        </field>
        <field name="status">
            <property name="ModeCreate">EDITABLE</property>
            <property name="ModeEdit">EDITABLE</property>
            <property name="Attribute">Status</property>
        </field>
        <field name="createGroup" character="Group">
            <property name="GroupCount">2</property>
        </field>
        <field name="creator">
            <property name="SelectAlternateOID">linkto[Creator].oid</property>
            <property name="Attribute">Creator</property>
            <property name="HRef">${COMMONURL}/MenuTree.jsp</property>
            <property name="ShowTypeIcon">true</property>
        </field>
        <field name="created">
            <property name="Attribute">Created</property>
        </field>
        <field name="modifyGroup" character="Group">
            <property name="GroupCount">2</property>
        </field>
        <field name="modifier">
            <property name="SelectAlternateOID">linkto[Modifier].oid</property>
            <property name="Attribute">Modifier</property>
            <property name="HRef">${COMMONURL}/MenuTree.jsp</property>
            <property name="ShowTypeIcon">true</property>
        </field>
        <field name="modified">
            <property name="Attribute">Modified</property>
        </field>
    </definition>
</ui-form>
