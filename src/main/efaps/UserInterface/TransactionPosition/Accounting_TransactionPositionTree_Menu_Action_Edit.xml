<?xml version="1.0" encoding="UTF-8"?>



<ui-command xmlns="http://www.efaps.org/xsd"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">
    <uuid>902cef19-918e-4170-abc5-67c0245c3586</uuid>
    <file-application>eFapsApp-Accounting</file-application>
    

    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Accounting_TransactionPositionTree_Menu_Action_Edit</name>

        <access>
            <role>Accounting_Admin</role>
            <role>Accounting_Manager</role>
        </access>

        

        <target>
            <form>Accounting_TransactionPositionForm</form>
            <execute program="org.efaps.esjp.common.uiform.Edit" />
            <validate program="org.efaps.esjp.accounting.transaction.Transaction" method="validateEdit4CheckAmount"/>
            <trigger program="org.efaps.esjp.admin.access.AccessCheck4UI"
                method="check4Status"
                name="Accounting_TransactionPositionTree_Menu_Action_Edit.UI_ACCESSCHECK"
                event="UI_ACCESSCHECK">
                <property name="Status">Open</property>
                <property name="Select4Instance">linkto[TransactionLink].instance</property>
            </trigger>
        </target>

        

        <property name="Target">modal</property>
        <property name="TargetMode">edit</property>
        <property name="WindowHeight">460</property>
        <property name="WindowWidth">615</property>
    </definition>
</ui-command>
