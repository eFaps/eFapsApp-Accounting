<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<ui-command xmlns="http://www.efaps.org/xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">

    <uuid>f8493bf8-e43d-4fb5-9e58-02a5c7facbc8</uuid>
    <file-application>eFapsApp-Accounting</file-application>
    

    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Accounting_TransactionTree_Menu_Action_SetStatusClosed</name>
        <target>
            <execute program="org.efaps.esjp.admin.datamodel.StatusValue"
                method="setStatus">
                <property name="Status">Closed</property>
            </execute>
            <trigger program="org.efaps.esjp.admin.access.AccessCheck4UI"
                method="check4Status"
                name="Accounting_TransactionTree_Menu_Action_SetStatusClosed.UI_ACCESSCHECK"
                event="UI_ACCESSCHECK">
                <property name="Status">Open</property>
            </trigger>
        </target>
        <property name="AskUser">true</property>
        <property name="Submit">true</property>
        <property name="SubmitSelectedRows">-1</property>
    </definition>

</ui-command>
