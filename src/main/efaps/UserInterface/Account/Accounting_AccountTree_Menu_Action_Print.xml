<?xml version="1.0" encoding="UTF-8"?>




<ui-command xmlns="http://www.efaps.org/xsd"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">
    <uuid>3cd4508f-937b-4578-8d11-28c4e111428b</uuid>
    <file-application>eFapsApp-Accounting</file-application>
    
    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Accounting_AccountTree_Menu_Action_Print</name>
        <access>
            <role>Accounting_Admin</role>
            <role>Accounting_Manager</role>
        </access>
        <target>
            <form>Accounting_Report_PrintForm</form>
            <execute program="org.efaps.esjp.common.jasperreport.StandartReport">
                <property name="JasperReport">Accounting_Account</property>
                <property name="DataSourceClass">org.efaps.esjp.accounting.report.AccountDataSource</property>
            </execute>
            <trigger program="org.efaps.esjp.accounting.Account"
                name="Accounting_AccountTree_Menu_Action_Print.UI_ACCESSCHECK"
                method="accessCheckOnSummary" event="UI_ACCESSCHECK">
            </trigger>
        </target>
        <property name="Target">modal</property>
        <property name="TargetMode">create</property>
        <property name="TargetShowFile">true</property>
    </definition>
</ui-command>
