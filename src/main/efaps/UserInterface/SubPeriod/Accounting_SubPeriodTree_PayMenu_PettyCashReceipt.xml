<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<ui-command xmlns="http://www.efaps.org/xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">
    <uuid>5a03e3ce-dcc9-43e3-8c02-d4ec6a3a30f3</uuid>
    <file-application>eFapsApp-Accounting</file-application>
    
    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Accounting_SubPeriodTree_PayMenu_PettyCashReceipt</name>
        <access>
            <role>Accounting_Admin</role>
            <role>Accounting_Manager</role>
        </access>
        <target>
            <menu>Accounting_SubPeriodTree_PayMenu_PettyCashReceipt_Menu</menu>
            <table>Accounting_SubPeriodPettyCashReceiptTable</table>
            <evaluate program="org.efaps.esjp.accounting.SubPeriod" method="getPettyCashReceipt">
                <property name="PettyCashBalanceStatus">Verified</property>
                <property name="Type01">Sales_PettyCashReceipt</property>
                <property name="Type02">Sales_IncomingCreditNote</property>
                <property name="StatusGroup01">Sales_PettyCashReceiptStatus</property>
                <property name="Status01">Closed</property>
                <property name="StatusGroup02">Sales_IncomingCreditNoteStatus</property>
                <property name="Status02">Paid</property>
            </evaluate>
        </target>
        <property name="Target">content</property>
        <property name="TargetShowCheckBoxes">true</property>
        <property name="Label">Accounting_PeriodTree_PayMenu_PettyCashReceipt.Label</property>
        <property name="TargetTitle">Accounting_PeriodTree_PayMenu_PettyCashReceipt.Title</property>
    </definition>
</ui-command>
